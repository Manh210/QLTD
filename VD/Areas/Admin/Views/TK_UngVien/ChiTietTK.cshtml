@model VD.Models.UngVien

@{
    ViewBag.Title = "Chi tiết Ứng viên";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section naviheader{
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
        </li>
        <li class="nav-item d-none d-sm-inline-block">
            <a href="/admin/home" class="nav-link">Trang chủ</a>
        </li>
        <li class="nav-item d-none d-sm-inline-block">
            <a href="/admin/TK_UngVien" class="nav-link">Quản lý tài khoản ứng viên</a>
        </li>
        <li class="nav-item d-none d-sm-inline-block">
            <a href="#" class="nav-link">Chi tiết</a>
        </li>
    </ul>
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Chi tiết</h1>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>


<!-- Main content -->
<section class="content">

    <!-- Default box -->
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">Thông tin ứng viên</h3>
        </div>
        <div class="card-body">
            <div class="bs-stepper-content">
                <!-- your steps content here -->
                <div id="logins-part" class="content active dstepper-block" role="tabpanel" aria-labelledby="logins-part-trigger">
                    <div class="form-group">
                        <label for="exampleInputEmail1">ID ứng viên</label>
                        <input class="form-control" id="ID_UV" name="ID_UV" type="text" value="@Model.ID_UV">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Họ và tên</label>
                        <input class="form-control" id="HoTen" name="HoTen" type="text" value="@Model.HoTen">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Số điện thoại</label>
                        <input class="form-control" id="SDT" name="SDT" type="text" value="@Model.SDT">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Ngày sinh</label>
                        <input class="form-control" id="DOB" name="DOB" type="text" value="@Model.DOB">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Giới tính</label>
                        <input class="form-control" id="GioiTinh" name="GioiTinh" type="text" value="@Model.GioiTinh">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Email</label>
                        <input class="form-control" id="Email" name="Email" type="email" value="@Model.Email">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">CV</label>
                        <input class="form-control" id="CV" name="CV" type="text" value="@Model.CV">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Sơ yếu lý lịch</label>
                        <input class="form-control" id="SYLL" name="SYLL" type="text" value="@Model.SYLL">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Địa điểm</label>
                        <input class="form-control" id="SYLL" name="SYLL" type="text" value="@Model.DiaDiem.TenDD">
                    </div>
                    <div class="form-group">
                        <a href="/Admin/TK_UngVien" class="btn btn-danger">Quay lại</a>
                    </div>
                </div>
            </div>
            <!-- /.card-body -->
            <div class="card-footer">

            </div>
            <!-- /.card-footer-->
        </div>
        <!-- /.card -->

</section>
<!-- /.content -->
